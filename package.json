{
  "name": "portsense",
  "version": "1.0.0",
  "description": "AI-powered maritime container tracking SaaS platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "monitor": "tsx scripts/monitor-containers.ts",
    "test-ai": "tsx scripts/test-ai-providers.ts",
    "simulate-realtime": "tsx scripts/simulate-realtime-updates.ts",
    "db:migrate": "supabase db push",
    "db:seed": "tsx scripts/seed-data.ts",
    "db:reset": "supabase db reset",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "cross-env ANALYZE=true next build",
    "postinstall": "prisma generate || true"
